<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lcm.dao.business.IY23SystemParamDao" >
  <resultMap id="resultMap" type="com.lcm.entity.pojo.Y23SystemParam" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="param" property="param" jdbcType="VARCHAR" />
    <result column="code" property="code" jdbcType="VARCHAR" />
    <result column="type" property="type" jdbcType="VARCHAR" />
    <result column="is_delete" property="isDelete" jdbcType="BIT" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
    <result column="modify_time" property="modifyTime" jdbcType="TIMESTAMP" />
  </resultMap>
  
  <select id="select" resultMap="resultMap">
  	select * from y23_system_param where is_delete=0
  </select>
  
  <select id="selectParam" resultMap="resultMap">
  	select * from y23_system_param where is_delete=0
  	<if test="code != null">
  	and code = #{code}
  	</if>
  </select>
  
  <update id="update" parameterType="com.lcm.entity.pojo.Y23SystemParam" >
    update y23_system_param
    <set >
      <if test="param != null" >
        param = #{param},
      </if>
      <if test="type != null" >
        type = #{type},
      </if>
      <if test="value != null" >
        value = #{value},
      </if>
       modify_time = now()
    </set>
    where code = #{code}
  </update>
</mapper>